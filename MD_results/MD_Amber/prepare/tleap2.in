source leaprc.protein.ff14SB
source leaprc.gaff2
source leaprc.water.tip3p
loadamberparams UNL.frcmod
loadoff UNL.lib
prot = loadpdb 6vzk.pqr
LIG1 = loadmol2 UNL.mol2
